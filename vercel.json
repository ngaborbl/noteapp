{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": ".",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "headers": {
        "cache-control": "s-maxage=1, stale-while-revalidate"
      },
      "dest": "/$1"
    }
  ],
  "github": {
    "enabled": true,
    "silent": true
  }
}